package com.synflow.led;

task LedDriver {
	out u8 leds;
	
	void loop() {
		leds.write(0b1111_1111);
		leds.write(0b1111_1110);
		leds.write(0b1111_1100);
		leds.write(0b1111_1001);
		leds.write(0b1111_0011);
		leds.write(0b1110_0111);
		leds.write(0b1100_1111);
		leds.write(0b1001_1111);
		leds.write(0b0011_1111);
		leds.write(0b0111_1111);
		leds.write(0b1111_1111);
	}
}
