package com.synflow.hello;

task WordToDisplay {
	out char character;

	const char message[11] = {'H', 'E', 'L', 'L', 'O', ' ', 'W', 'O', 'R', 'L', 'D'};

	void loop() {
		for (u4 i = 0; i != 12; i++) {
			character.write(message[i]);
		}
	}

}
