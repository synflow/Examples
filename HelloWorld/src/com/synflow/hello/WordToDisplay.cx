package com.synflow.hello;

task WordToDisplay {
	out char character;

	const char message[12] = {'H', 'e', 'l', 'l', 'o', ' ', 'w', 'o', 'r', 'l', 'd', '!'};

	void loop() {
		for (u4 i = 0; i != 13; i++) {
			character.write(message[i]);
		}
	}

}
